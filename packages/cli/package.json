{
    "name": "@reefguide/cli",
    "version": "0.1",
    "description": "Work in progress CLI for ReefGuide",
    "main": "build/src/index.js",
    "bin": {
        "reefguide-cli": "build/src/index.js"
    },
    "scripts": {
        "build": "tsc",
        "prestart": "pnpm run build",
        "start": "node build/src/index.js",
        "watch": "tsc -w",
        "lint": "gts lint",
        "type-check": "tsc --noEmit",
        "format:write": "prettier --write \"{src,test}/**/*.{ts,js,json,html,scss,md}\" \"!**/node_modules/**\" \"!**/dist/**\"",
        "format:check": "prettier --check \"{src,test}/**/*.{ts,js,json,html,scss,md}\" \"!**/node_modules/**\" \"!**/dist/**\"",
        "clean": "gts clean",
        "fix": "gts fix"
    },
    "keywords": [],
    "author": "AIMS",
    "license": "MIT",
    "dependencies": {
        "@reefguide/types": "workspace:*",
        "axios": "^1.9.0",
        "commander": "^14.0.0",
        "dotenv": "^16.5.0",
        "env-cmd": "^10.1.0",
        "jwt-decode": "^4.0.0",
        "read": "^4.1.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0"
    }
}
