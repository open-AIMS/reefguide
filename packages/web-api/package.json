{
    "name": "@reefguide/web-api",
    "version": "0.1",
    "description": "REST API to support ReefGuide AIMS.",
    "main": "src/index.ts",
    "scripts": {
        "start": "env-cmd ts-node --files src/index.ts",
        "watch": "tsc -w",
        "dev": "env-cmd ts-node-dev --files src/index.ts",
        "build": "tsc",
        "lint": "gts lint",
        "test": "jest --detectOpenHandles",
        "type-check": "tsc --noEmit",
        "format:write": "prettier --write \"{src,test}/**/*.{ts,js,json,html,scss,md}\" \"!**/node_modules/**\" \"!**/dist/**\"",
        "format:check": "prettier --check \"{src,test}/**/*.{ts,js,json,html,scss,md}\" \"!**/node_modules/**\" \"!**/dist/**\"",
        "clean": "gts clean",
        "fix": "gts fix",
        "local-keys": "node scripts/localKeys.js",
        "start-web-api-docker": "ts-node -T --files src/index.ts"
    },
    "keywords": [],
    "author": "AIMS",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-ec2": "^3.692.0",
        "@aws-sdk/client-ecs": "^3.687.0",
        "@aws-sdk/client-s3": "^3.689.0",
        "@aws-sdk/s3-request-presigner": "^3.689.0",
        "@cloudcomponents/cdk-static-website": "^2.2.0",
        "@codegenie/serverless-express": "^4.15.0",
        "@reefguide/db": "workspace:*",
        "@reefguide/types": "workspace:*",
        "@types/aws-lambda": "^8.10.145",
        "aws-cdk-lib": "2.158.0",
        "axios": "^1.7.7",
        "bcryptjs": "^2.4.3",
        "constructs": "^10.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "env-cmd": "^10.1.0",
        "express": "^4.21.1",
        "express-async-errors": "^3.1.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "jwt-decode": "^4.0.0",
        "morgan": "^1.10.0",
        "node-cron": "^4.1.1",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "source-map-support": "^0.5.21",
        "winston": "^3.17.0",
        "zod": "^3.23.8",
        "zod-express-middleware": "^1.4.0"
    },
    "devDependencies": {
        "@aws-sdk/client-secrets-manager": "^3.654.0",
        "@types/bcrypt": "^5.0.2",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/node-cron": "^3.0.11",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/supertest": "^6.0.2",
        "aws-cdk": "2.158.0",
        "cdk-dia": "^0.11.0",
        "esbuild": "^0.23.1",
        "jest": "^29.7.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0",
        "zod-prisma-types": "^3.1.8"
    }
}
