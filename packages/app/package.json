{
  "name": "@reefguide/app",
  "version": "0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,html,scss,md}\" \"!**/node_modules/**\" \"!**/dist/**\"",
    "format:write": "prettier --write \"src/**/*.{ts,js,json,html,scss,md}\" \"!**/node_modules/**\" \"!**/dist/**\"",
    "sourcemaps": "pnpm run build && ./node_modules/@sentry/cli/bin/sentry-cli sourcemaps inject dist/adria-app && ./node_modules/@sentry/cli/bin/sentry-cli --url ${SENTRY_URL} sourcemaps --org none --project none upload dist/adria-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.4",
    "@angular/cdk": "^20.2.0",
    "@angular/common": "^20.3.4",
    "@angular/compiler": "^20.3.4",
    "@angular/core": "^20.3.4",
    "@angular/forms": "^20.3.4",
    "@angular/material": "^20.2.0",
    "@angular/platform-browser": "^20.3.4",
    "@angular/platform-browser-dynamic": "^20.3.4",
    "@angular/router": "^20.3.4",
    "@ngx-env/builder": "^20.1.1",
    "@reefguide/db": "workspace:*",
    "@reefguide/types": "workspace:*",
    "@sentry/angular": "^10.4.0",
    "apache-arrow": "^21.0.0",
    "fuzzysort": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "ol": "^10.6.1",
    "parquet-wasm": "^0.6.1",
    "proj4": "^2.19.5",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "vega-embed": "^7.0.2",
    "zod": "^3.25.62"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.4",
    "@sentry/cli": "^2.53.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0"
  }
