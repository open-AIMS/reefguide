        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/supertest": "^6.0.2",
        "aws-cdk": "2.158.0",
        "cdk-dia": "^0.11.0",
        "esbuild": "^0.23.1",
